    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.17.3",
    "graphql": "^0.11.7",
    "graphql-date": "^1.0.3",
    "graphql-subscriptions": "^0.5.5",
    "graphql-tools": "^2.12.0",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.14",
    "merge-graphql-schemas": "^1.4.0",
