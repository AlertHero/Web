{
  "name": "alerthero-server",
  "version": "1.0.0-beta",
  "description": "AlertHero Server",
  "main": "server.js",
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server.js",
